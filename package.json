{
  "name": "sugarcode",
  "displayName": "SugarCode",
  "description": "MED+梦龙是RBQ",
  "version": "1.1.4",
  "engines": {
    "vscode": "*"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "SugarCode",
        "extensions": [
          ".sgc"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./logo.png",
          "dark": "./logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "SugarCode",
        "scopeName": "source.SugarCode",
        "path": "./syntaxes/SugarCode.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "SugarCode",
        "path": "./snippets/SugarCode.snippets.code-snippets"
      }
    ]
  }
}
